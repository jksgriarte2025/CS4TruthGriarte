package ex03truthgriarte;

/**
 *
 * @author TRUTH
 */
public class Song {
    String songname, singer, album;
    double duration;
    
    public Song(String songName, String songSinger, String songAlbum,
            double songDuration){
        songname = songName;
        singer = songSinger;
        album = songAlbum;
        duration = songDuration;
        //song duration is seconds
    }
}
