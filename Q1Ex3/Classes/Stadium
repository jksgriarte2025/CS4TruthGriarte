package ex03truthgriarte;

/**
 *
 * @author TRUTH
 */
public class Stadium {
    String stadiumname, location;
    double capacity, comfort;
    
    public Stadium(String stadiumName, String stadiumLoc,double stadiumCap, 
            double stadiumComf) {
    stadiumname = stadiumName;
    location = stadiumLoc;
    capacity = stadiumCap;
    comfort = stadiumComf;
}
}
